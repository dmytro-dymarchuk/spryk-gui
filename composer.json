{
  "name": "spryker-sdk/spryk-gui",
  "type": "library",
  "description": "SprykGui module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker-sdk/spryk": "dev-master",
    "spryker/config": "^3.1.3",
    "spryker/development": "^3.12.0",
    "spryker/graph": "^3.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/zend": "^2.0.0"
  },
  "require-dev": {
    "spryker/application": "*",
    "spryker/code-sniffer": "*",
    "spryker/propel": "*",
    "spryker/testify": "*",
    "spryker/zed-navigation": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:spryker-sdk/spryk.git"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
